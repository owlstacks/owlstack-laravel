{
    "name": "synglify/synglify-laravel",
    "description": "Laravel integration for Synglify â€” publish content to Telegram, X (Twitter), and Facebook from your Laravel application.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "synglify",
        "social-media",
        "telegram",
        "twitter",
        "x",
        "facebook",
        "content-syndication",
        "publishing"
    ],
    "authors": [
        {
            "name": "Ali Hesari",
            "email": "alihesari.com@gmail.com",
            "homepage": "https://alihesari.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "synglify/synglify-core": "@dev",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Synglify\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Synglify\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Synglify\\Laravel\\SynglifyServiceProvider"
            ],
            "aliases": {
                "Synglify": "Synglify\\Laravel\\Facades\\Synglify"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../synglify-core",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
